    "start:no_pug": "webpack-dev-server",
    "build:no_pug": "\"npm run clear:dist\" && webpack -p",
    "start": "concurrently \"npm run clear:html\" \"npm run pug:watch\" \"webpack-dev-server\" ",
    "build": "npm run clear:dist && npm run pug:build && webpack -p && npm run clear:html",
    "pug:build": "pug -o src/ src/templates/ -P",
    "pug:watch": "pug -o src/ src/templates/ -P -w",
    "clear": "npm run clear:dist && npm run clear:html",
    "clear:dist": "rimraf dist",
    "clear:html": "rimraf src/*.html"
